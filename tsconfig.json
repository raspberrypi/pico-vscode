{
	"compilerOptions": {
		"module": "node16",
		"target": "ES2022",
		"lib": [
			"ES2022"
		],
		"sourceMap": false,
		"inlineSourceMap": true,
		"rootDir": "src",
		"outDir": "dist",
		"strict": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"moduleResolution": "node16",
		"esModuleInterop": true,
	}
}